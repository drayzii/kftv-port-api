{
  "name": "kftv-studio-port",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && babel server -d build",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate:dev": "sequelize db:migrate --env development",
    "db-migrate:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "dev": "set NODE_ENV=development && nodemon --exec babel-node ./server/server.js",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "start": "export NODE_ENV=production npm run build && npm run db-migrate && node ./build/server.js",
    "test": "export NODE_ENV=test && npm run db-migrate:test && nyc mocha --require @babel/register ./server/test/*.js --timeout 10000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drayzii/kftv-studio-port.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drayzii/kftv-studio-port/issues"
  },
  "homepage": "https://github.com/drayzii/kftv-studio-port#readme",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "googleapis": "^52.1.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.8",
    "open": "^7.0.4",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "server-destroy": "^1.0.1",
    "youtube-video-api": "^0.3.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "mocha": "^8.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  }
}
